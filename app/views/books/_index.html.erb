<h1>User info</h1>
<%= image_tag current_user.get_profile_image(100,100) %>
<div>
<p>name</p>
<%= current_user.name %>
<p>introduction</p>
<%= current_user.introduction %>
</div>

<p><%= link_to "User編集", edit_user_path(current_user.id), method: :get %></p>

<h1>New book</h1>
<%= form_with model: @newBook, url:books_path, method: :post do |f| %>
 <p>Title</p>
 <%= f.text_field :title %>

 <p>Opinion</p>
 <%= f.text_area :body %>
 
 <%= f.submit 'Create Book' %>
<%end%>
